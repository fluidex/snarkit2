    "chai": "^4.3.4",
    "commander": "^7.2.0",
    "ffiasm": "^0.1.1",
    "ffjavascript": "^0.2.35",
    "r1csfile": "0.0.16",
    "shelljs": "^0.8.4",
    "snarkjs": "^0.3.60",
